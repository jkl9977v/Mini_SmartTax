<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.//EN"
     "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="SmartTax.mapper.CheckMapper">

<select id="userIdCheck" parameterType="string" resultType="integer">
select 1 from users where user_id=#{userId}
</select>

<select id="userEmailCheck" parameterType="string" resultType="integer">
select 1 from users where user_email=#{userEmail}
</select>

</mapper>